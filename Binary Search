#include<bits/stdc++.h>
using namespace std;
vector<int> vec;

int binary_search(int a){
    int low=0;
    int high=vec.size()-1;

    while(low<=high){

        int mid=(low+high)/2;

        if(vec[mid]==a){
            return mid;
        }
        else if(a>vec[mid])
        {
            low=mid+1;
        }
        else if(a<vec[mid]){
            high=mid-1;
        }
        else{
            return -1;
        }
    }
}

int main(){

    int n,x;
    cin>>n;

    for(int i=0;i<n;i++)
    {
        cin>>x;
        vec.push_back(x);
    }

    int a;
    cin>>a;

    int result = binary_search(a);

    cout << result;

    return 0;
}
